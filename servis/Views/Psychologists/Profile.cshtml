
@model servis.Models.Psychologist


@{
    ViewData["Title"] = "Аккаунт создать";
}

<div class="row">
    <div class="col-md-4">
          <form asp-action="Profile">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ID" />
             <input type="hidden" asp-for="Name" />
 <input type="hidden" asp-for="LastName" />
 <input type="hidden" asp-for="Year" />
 <input type="hidden" asp-for="Info" />
 <input type="hidden" asp-for="Price" />
  <input type="hidden" asp-for="Methods_objId" />
 <input type="hidden" asp-for="Specialization_objId" />
 <input type="hidden" asp-for="Email" />
 <input type="hidden" asp-for="Phone" />
  <input type="hidden" asp-for="Photo" />
 <input type="hidden" asp-for="Profile" />
            <h2>Создать новый аккаунт.</h2>
            <hr />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-floating">
                <input asp-for="Password" class="form-control" autocomplete="new-password" aria-required="true" />
                <label asp-for="Password"></label>
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>
           <div class="form-group">
                <input type="submit" value="Зарегестрировать" class="btn btn-primary" />
            </div>
        </form>
    </div>
   
</div>

<div>
    <a asp-action="Index">Обратно</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
